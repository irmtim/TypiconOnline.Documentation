<?xml version="1.0" encoding="UTF-8"?>
<rule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file:///E:/Programming/Documentation/TypiconOnline.Documentation/XML/Rules/RulesSchema.xsd">
	<switch>
		<expression>
			<daysfromeaster><date>--04-07</date></daysfromeaster>
		</expression>
		<case>
			<values>
				<int>-25</int>
			</values>
			<action>
				<modifyday daymove="-2" priority="3"/>
				<service time="17.30" name="9-й час.  Вечерня. Малое повечерие." isdaybefore="true"/>
				<service time="06.00" name="Полунощница. Утреня."></service>
				<service time="08.00" name="Часы. Изобразительны. Вечерня."></service>
			</action>
		</case>
		<default>
			<modifyday daymove="0" priority="3"/>
			<service time="17.30" name="Великое повечерие." isdaybefore="true"/>
			<service time="06.00" name="Полунощница. Утреня. Часы."></service>
			<service time="09.00" name="Литургия Преждеосвященных Даров."></service>
		</default>
	</switch>
</rule>
