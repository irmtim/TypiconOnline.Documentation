<?xml version="1.0" encoding="UTF-8"?>
<rule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file:///E:/Programming/Documentation/TypiconOnline.Documentation/XML/Rules/RulesSchema.xsd">
	<switch>
		<expression>
			<getclosestday dayofweek="суббота" weekcount="-1">
				<date>--11-08</date>
			</getclosestday>
		</expression>
		<case>
			<values>
				<date>--11-04</date>
			</values>
			<action>
				<modifyday asaddition="true">
					<getclosestday dayofweek="суббота" weekcount="-2">
						<date>--11-08</date>
					</getclosestday>
				</modifyday>
			</action>
		</case>
		<default>
			<modifyday asaddition="true">
				<getclosestday dayofweek="суббота" weekcount="-1">
					<date>--11-08</date>
				</getclosestday>
			</modifyday>
		</default>
	</switch>
	<worship time="17.30" isdaybefore="true">
		<name>
			<item language="cs-ru">9-й час.  Вечерня. Малое повечерие.</item>
		</name>
	</worship>
	<worship time="06.00">
		<name>
			<item language="cs-ru">Полунощница. Утреня. Часы.</item>
		</name>
	</worship>
	<worship time="08.00">
		<name>
			<item language="cs-ru">Божественная литургия.</item>
		</name>
		<additionalname>
			<item language="cs-ru">Панихида по усопшим.</item>
		</additionalname>
	</worship>
</rule>
