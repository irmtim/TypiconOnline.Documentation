<rule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file:///E:/Programming/Documentation/TypiconOnline.Documentation/XML/Rules/RulesSchema.xsd">
	<switch>
		<expression>
			<daysfromeaster>
				<date/>
			</daysfromeaster>
		</expression>
		<case>
			<values>
				<int>-48</int>
				<int>-41</int>
				<int>-34</int>
				<int>-27</int>
				<int>-20</int>
				<int>-13</int>
			</values>
			<action>
				<worship mode="DayBefore">
					<time>17.30</time>
					<name>9-й час. Вечерня. Малое повечерие.</name>
				</worship>
			</action>
		</case>
		<default>
			<worship mode="DayBefore">
				<time>17.30</time>
				<name>Великое повечерие.</name>
			</worship>
		</default>
	</switch>
	<worship>
		<time>06.00</time>
		<name>Полунощница. Утреня.</name>
	</worship>
	<worship>
		<time>08.00</time>
		<name>Часы. Изобразительны. Вечерня.</name>
	</worship>
</rule>
